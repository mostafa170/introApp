<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/iv_contact_iconList"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginTop="5dp"
            android:src="@drawable/ic_woman_icon_plain_in_circle"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_contact_nameList"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Alamat"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/iv_contact_iconList"
            app:layout_constraintStart_toStartOf="@+id/iv_contact_iconList"
            app:layout_constraintTop_toBottomOf="@+id/iv_contact_iconList" />

        <TextView
            android:id="@+id/tv_contact_statusList"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="avilable"
            app:layout_constraintEnd_toEndOf="@+id/iv_contact_iconList"
            app:layout_constraintStart_toStartOf="@+id/iv_contact_iconList"
            app:layout_constraintTop_toBottomOf="@+id/tv_contact_nameList" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>